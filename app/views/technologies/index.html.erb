<div class="container">
  <h1  style="font-family:arial">Listing All technologies</h1>
  <div class="jumbotron">
    <table class="table table-striped">
      <tr>
        <th>Name</th>
        <th></th>
        <th></th>
      </tr>
     <% @technology.each do |technology| %>
      <tr>
          <td><%= technology.name %></td>
         
          <td><%= link_to 'Edit', edit_technology_path(technology) %></td>
          <td><%= link_to 'Delete', technology_path(technology),method: :delete, data: {confirm: 'Are you sure?'} %></td>
      </tr>
      <% end %>
    </table>
   <%= link_to 'Add technology', new_technology_path,class:"btn btn-primary btn-md" %>
  
</div>